{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=bitoauto;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
    "PasswordRules": {
      "RequiredLength": "8",
      "RequireNonAlphanumeric": "True",
      "RequireUppercase": "True",
      "RequireLowercase": "True",
      "RequireDigit": "True"
    },
    "Lockout": {
      "LockoutTimeSpanMinutes": "10",
      "MaxFailedAccessAttempts": "5"
    },
  "AllowedHosts": "*",
  "AutoSeed": "True"
}
